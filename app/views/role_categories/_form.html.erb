
<%= render partial: 'layouts/form_print_errors', locals: {model: role_category} %>

<%= form_with(model: role_category, local: true) do |form| %>
  <div class="field">
    <%= form.label :mname %>
    <%= form.text_field :mname %>
  </div>

  <div class="field">
    <%= form.label :note %>
    <%= form.text_area :note %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
