
<%= render partial: 'layouts/form_print_errors', locals: {model: sex} %>

<%= form_with(model: sex, local: true) do |form| %>
  <div class="field">
    <%= form.label :iso5218 %>
    <%= form.number_field :iso5218 %>
  </div>

  <div class="field">
    <%= form.label :note %>
    <%= form.text_area :note %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
