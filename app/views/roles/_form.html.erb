
<%= render partial: 'layouts/form_print_errors', locals: {model: role} %>

<%= form_with(model: role, local: true) do |form| %>
  <div class="field">
    <%= form.label :name %>
    <%= form.text_field :name %>
  </div>

  <div class="field">
    <%= form.label :uname %>
    <%= form.text_field :uname %>
  </div>

  <div class="field">
    <%= form.label :role_category_id %>
    <%= form.text_field :role_category_id %>
  </div>

  <div class="field">
    <%= form.label :weight %>
    <%= form.number_field :weight %>
  </div>

  <div class="field">
    <%= form.label :note %>
    <%= form.text_area :note %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
